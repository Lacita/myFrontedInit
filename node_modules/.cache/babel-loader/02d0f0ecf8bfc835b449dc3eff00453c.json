,IAAIC,IAAJ,GAAWC,QAAX,EAAhB,cAAyCF,GAAG,GAAG,CAA/C;UAAA,CAArB,CADD;UAELG,IAAI,EAAE;QAFD,CAZS;QAgBhBC,KAAK,EAAE;UACLX,IAAI,EAAE,OADD;UAELU,IAAI,EAAE;QAFD,CAhBS;QAoBhBE,MAAM,EAAE,CACN;UACEF,IAAI,EAAE,KADR;UACe;UACbV,IAAI,EAAEvB,OAAO,CAACG,WAFhB;UAE6B;UAC3BiC,MAAM,EAAE,IAHV;UAGgB;UACdR,IAAI,EAAE5B,OAAO,CAACC,QAJhB;UAKEoC,KAAK,EAAErC,OAAO,CAACI,SALjB;UAK4B;UAC1BkC,SAAS,EAAE;YAETC,KAAK,EAAEvC,OAAO,CAACK;UAFN,CANb;UAWE;UACAmC,SAAS,EAAE;YACTH,KAAK,EAAEtC,OAAO,CAAC0C,OAAR,CAAgBC,cAAhB,CAA+B,CAA/B,EAAkC,CAAlC,EAAqC,CAArC,EAAwC,CAAxC,EAA2C,CAChD;cACEC,MAAM,EAAE,CADV;cAEEN,KAAK,EAAE;YAFT,CADgD,EAKhD;cACEM,MAAM,EAAE,CADV;cAEEN,KAAK,EAAE;YAFT,CALgD,CAA3C;UADE;QAZb,CADM;MApBQ,CAAlB;IAgDD;;IAEDxC,SAAS,CAAC,YAAM;MACdY,OAAO,GAAGV,OAAO,CAAC6C,IAAR,CAAaC,QAAQ,CAACC,cAAT,CAAwB,SAAxB,CAAb,CAAV,CADc,CAGd;;MACApC,UAAU;MACVF,aAAa;IACd,CANQ,CAAT","names":["reactive","ref","onMounted","watch","echarts","options","userData","showAxle","echartsType","dataColor","lineWidth","val","prevCount","renderEcharts","myChart","randomData","Array","fill","map","Math","ceil","random","setOption","title","text","tooltip","trigger","axisPointer","type","animation","label","backgroundColor","xAxis","data","it","idx","Date","getMonth","name","yAxis","series","smooth","color","lineStyle","width","areaStyle","graphic","LinearGradient","offset","init","document","getElementById"],"sources":["D:/vue3/vue3_ts_antd/src/views/echart/basics.vue"],"sourcesContent":["<template>\r\n  <div class=\"page\">\r\n    <el-form label-position=\"top\">\r\n      <el-form-item label=\"操作：\">\r\n        <div class=\"btns\">\r\n          <el-button @click=\"randomData\">重新生成数据</el-button>\r\n        </div>\r\n      </el-form-item>\r\n      <el-form-item label=\"图表样式：\">\r\n        <el-radio-group v-model=\"options.echartsType\">\r\n          <el-radio label=\"line\">折线图</el-radio>\r\n          <el-radio label=\"bar\">柱状图</el-radio>\r\n        </el-radio-group>\r\n      </el-form-item>\r\n      <!-- <el-form-item label=\"显示轴标线：\">\r\n        <el-switch v-model=\"options.showAxle\" />\r\n      </el-form-item> -->\r\n      <el-form-item label=\"选择线条颜色：\">\r\n        <el-color-picker v-model=\"options.dataColor\" />\r\n      </el-form-item>\r\n      <el-form-item label=\"选择线折线宽度：\">\r\n        <el-slider v-model=\"options.lineWidth\" :min=\"1\" :max=\"20\" />\r\n      </el-form-item>\r\n    </el-form>\r\n    <div id=\"myChart\" :style=\"{ width: '800px', height: '500px' }\" />\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { reactive, ref } from \"@vue/reactivity\"\r\nimport { onMounted, watch } from \"@vue/runtime-core\";\r\nimport * as echarts from \"echarts\";\r\n\r\nconst options = reactive({\r\n  userData: {},\r\n  showAxle: true,\r\n  echartsType: \"line\",\r\n  dataColor: \"#1E75B4\", // 数据颜色\r\n  lineWidth: 3, // 线条宽度\r\n})\r\n\r\n// 监听,每一个数据发生变化时都重新渲染\r\nwatch(options, (val, prevCount) => {\r\n  renderEcharts()\r\n})\r\n\r\n\r\nlet myChart;\r\n\r\n// 生成随机数据\r\nconst randomData = () => {\r\n  options.userData = Array(30).fill().map(() => Math.ceil(Math.random() * 1000));\r\n}\r\n\r\n// 重新渲染dom\r\nfunction renderEcharts () {\r\n  myChart.setOption({\r\n    title: { text: \"网站近期访问量\" },\r\n    tooltip: {\r\n      trigger: \"axis\",\r\n      axisPointer: {\r\n        type: \"cross\",\r\n        animation: false,\r\n        label: {\r\n          backgroundColor: options.dataColor\r\n        }\r\n      }\r\n    },\r\n    xAxis: {\r\n      data: Array(30).fill().map((it, idx) => `${new Date().getMonth()}-${idx + 1}`),\r\n      name: \"日期\"\r\n    },\r\n    yAxis: {\r\n      type: \"value\",\r\n      name: \"浏览用户量\"\r\n    },\r\n    series: [\r\n      {\r\n        name: \"用户量\", // 折线名称\r\n        type: options.echartsType, // 数据显示类型\r\n        smooth: true, // 是否开启曲线效果\r\n        data: options.userData,\r\n        color: options.dataColor, // 数据颜色\r\n        lineStyle: {\r\n\r\n          width: options.lineWidth\r\n\r\n        },\r\n        // 渐变色\r\n        areaStyle: {\r\n          color: echarts.graphic.LinearGradient(0, 0, 0, 1, [\r\n            {\r\n              offset: 0,\r\n              color: \"rgba(93, 233, 214, 0.8)\"\r\n            },\r\n            {\r\n              offset: 1,\r\n              color: \"rgba(216, 118, 216,0.3)\"\r\n            }\r\n          ])\r\n        },\r\n      },\r\n    ],\r\n  });\r\n}\r\n\r\nonMounted(() => {\r\n  myChart = echarts.init(document.getElementById(\"myChart\"));\r\n\r\n  // 绘制图表\r\n  randomData()\r\n  renderEcharts()\r\n})\r\n\r\n\r\n\r\n\r\n</script>\r\n\r\n<style scoped lang='less'>\r\n.page {\r\n  display: flex;\r\n  height: 100%;\r\n\r\n  .el-form {\r\n    width: 250px;\r\n    max-height: 100%;\r\n    overflow-y: auto;\r\n    /deep/ .el-form-item__label {\r\n      height: 38px;\r\n    }\r\n    .el-slider {\r\n      width: 200px;\r\n      margin-left: 20px;\r\n    }\r\n  }\r\n}\r\n.btns {\r\n  /* margin-bottom: 20px; */\r\n}\r\n.chart {\r\n  height: 400px;\r\n}\r\n\r\n.aaa {\r\n  color: rgba(216, 118, 216, 0.8);\r\n}\r\n.mt-20 {\r\n  margin-top: 20px;\r\n}\r\n.mb-20 {\r\n  margin-bottom: 20px;\r\n}\r\n</style>"]}]}